{
  "name": "react-app-setup",
  "version": "1.0.0",
  "description": "Basic React app setup with Webpack 3, Redux, Redux-thunk, React-router 4, font-generator, scss, post-css, jest",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/klaytonfaria/react-app-setup.git"
  },
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server --port=8080 --progress",
    "build": "node ./node_modules/webpack/bin/webpack -p --optimize-dedupe",
    "serve": "node ./node_modules/http-server/bin/http-server -p 8008 ./public",
    "stats": "node ./node_modules/webpack/bin/webpack --progress -c --profile --json > stats.json",
    "readme": "readme"
  },
  "author": {
    "name": "Klayton Faria",
    "url": "https://github.com/klaytonfaria"
  },
  "username": "klaytonfaria",
  "dependencies": {
    "apollo-boost": "^0.1.12",
    "graphql": "^0.13.2",
    "history": "4.7.2",
    "npm": "^6.2.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "eslint": "5.2.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.3.1",
    "extract-text-webpack-plugin": "3.0.2",
    "fontgen-loader": "0.2.1",
    "http-server": "0.11.1",
    "json-loader": "0.5.7",
    "node-sass": "4.9.2",
    "postcss-loader": "2.1.6",
    "readme-generator": "1.0.1",
