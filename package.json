{
  "name": "react-app-setup",
  "version": "1.0.0",
  "description": "Basic React app setup with Webpack 3, Redux, Redux-thunk, React-router 4, font-generator, scss, post-css, jest",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/klaytonfaria/react-app-setup.git"
  },
  "scripts": {
    "start": "webpack --config ./webpack.config.js --mode=development --progress",
    "build": "webpack -p --optimize-dedupe",
    "serve": "webpack-dev-server --config ./webpack.config.js --mode=development --progress",
    "build:dev": "webpack --config ./webpack.config.js --progress --mode development",
    "build:prod": "webpack --config ./webpack.config.js --mode production",
    "stats": "webpack --config ./webpack.config.js --profile --mode production --progress --json > stats.json",
    "readme": "readme",
    "start:ssr": "next start",
    "build:ssr": "next build",
    "dev:ssr": "next"
  },
  "author": {
    "name": "Klayton Faria",
    "url": "https://github.com/klaytonfaria"
  },
  "username": "klaytonfaria",
  "dependencies": {
    "history": "4.7.2",
    "next": "7.0.2",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-redux": "5.1.0",
    "react-router": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "webpack-cli": "3.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "copy-webpack-plugin": "4.5.4",
    "css-loader": "1.0.1",
    "eslint": "5.8.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.4.0",
    "extract-css-chunks-webpack-plugin": "3.2.1",
    "fontgen-loader": "0.2.1",
    "json-loader": "0.5.7",
    "postcss-cssnext": "3.1.0",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.0",
    "postcss-nested": "4.1.0",
    "postcss-scss": "2.0.0",
